<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://iit.ru/portal:8888/portal"
	xmlns:tns="http://iit.ru/portal:8888/portal">

	<element name="NoticeRequest">
		<complexType>
			<sequence>
				<element maxOccurs="1" minOccurs="1" name="PredId" type="int" />
				<element maxOccurs="1" minOccurs="1" name="PersonId" type="int" />
				<element maxOccurs="1" minOccurs="1" name="DocId" type="long" />
			</sequence>
		</complexType>
	</element>

	<element name="NoticeResponse">
		<complexType>
			<sequence>
				<element maxOccurs="1" minOccurs="0" name="Notice" type="hexBinary" />
				<element maxOccurs="1" minOccurs="0" name="stage" type="int" />
				<element maxOccurs="1" minOccurs="0" name="NoticeType" type="string" />
				<element maxOccurs="1" minOccurs="1" name="description"
					type="string" />
				<element maxOccurs="1" minOccurs="1" name="code" type="int" />
			</sequence>
		</complexType>
	</element>
</schema>